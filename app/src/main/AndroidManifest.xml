<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid_19">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/coronavirus"
        android:label="@string/app_name"
        android:roundIcon="@drawable/coronavirus"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.mohammadKZ.CoronaVirus_COVID19_statistics.splashScreenActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.mohammadKZ.CoronaVirus_COVID19_statistics.HomeActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>